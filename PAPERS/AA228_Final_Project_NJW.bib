@article{chades_mdptoolbox:_2014,
	title = {{MDPtoolbox}: a multi-platform toolbox to solve stochastic dynamic programming problems},
	volume = {37},
	copyright = {{\textcopyright} 2014 The Authors},
	issn = {1600-0587},
	shorttitle = {{MDPtoolbox}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/ecog.00888},
	doi = {10.1111/ecog.00888},
	abstract = {Stochastic dynamic programming (SDP) or Markov decision processes (MDP) are increasingly being used in ecology to find the best decisions over time and under uncertainty so that the chance of achieving an objective is maximised. To date, few programs are available to solve SDP/MDP. We present MDPtoolbox, a multi-platform set of functions to solve Markov decision problems (MATLAB, GNU Octave, Scilab and R). MDPtoolbox provides state-of-the-art and ready to use algorithms to solve a wide range of MDPs. MDPtoolbox is easy to use, freely available and has been continuously improved since 2004. We illustrate how to use MDPtoolbox on a dynamic reserve design problem.},
	language = {en},
	number = {9},
	urldate = {2019-11-12},
	journal = {Ecography},
	author = {Chad{\`e}s, Iadine and Chapron, Guillaume and Cros, Marie-Jos{\'e}e and Garcia, Fr{\'e}d{\'e}rick and Sabbadin, R{\'e}gis},
	year = {2014},
	pages = {916--920},
	file = {Full Text PDF:C\:\\Users\\njwei\\Zotero\\storage\\QH4GGYYZ\\Chad{\`e}s et al. - 2014 - MDPtoolbox a multi-platform toolbox to solve stoc.pdf:application/pdf;Snapshot:C\:\\Users\\njwei\\Zotero\\storage\\3EI2ZFHW\\ecog.html:text/html}
}

